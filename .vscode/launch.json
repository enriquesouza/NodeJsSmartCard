    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Console Generator",
            "program": "${workspaceFolder}/console/generator/index.js",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Secret Express Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/services/secret/index.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "SmartPay Server",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/services/smartpay/index.js"
        }
    ]
}